<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.2</version>
    <date>2021-04-14T06:18:15Z</date>
    <groups>
        <group>
            <name>Templates</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Template_EDAC</template>
            <name>Template_EDAC</name>
            <description>Read EDAC data (ECC errors)</description>
            <groups>
                <group>
                    <name>Templates</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>EDAC - Raw</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>EDAC JSON</name>
                    <key>edac</key>
                    <history>0</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>EDAC - Raw</name>
                        </application>
                    </applications>
                </item>
            </items>
            <discovery_rules>
                <discovery_rule>
                    <name>Discover DIMM</name>
                    <type>DEPENDENT</type>
                    <key>edac.discovery</key>
                    <delay>0</delay>
                    <lifetime>7d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>{#DIMM} - Errors Correctable</name>
                            <type>DEPENDENT</type>
                            <key>edac.ce[{#DIMM}]</key>
                            <delay>0</delay>
                            <history>30d</history>
                            <application_prototypes>
                                <application_prototype>
                                    <name>EDAC</name>
                                </application_prototype>
                            </application_prototypes>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$.['{#DIMM}'].dimm_ce_count</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>edac</key>
                            </master_item>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&gt;{$EDAC_CE_THRESH}</expression>
                                    <name>{#DIMM} - Correctable errors detected: {ITEM.LASTVALUE}</name>
                                    <priority>AVERAGE</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>{#DIMM} - Location</name>
                            <type>DEPENDENT</type>
                            <key>edac.location[{#DIMM}]</key>
                            <delay>0</delay>
                            <history>1h</history>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <application_prototypes>
                                <application_prototype>
                                    <name>EDAC</name>
                                </application_prototype>
                            </application_prototypes>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$.['{#DIMM}'].dimm_location</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>edac</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#DIMM} - Size</name>
                            <type>DEPENDENT</type>
                            <key>edac.size[{#DIMM}]</key>
                            <delay>0</delay>
                            <history>1h</history>
                            <trends>0</trends>
                            <units>B</units>
                            <application_prototypes>
                                <application_prototype>
                                    <name>EDAC</name>
                                </application_prototype>
                            </application_prototypes>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$.['{#DIMM}'].size</parameter>
                                    </parameters>
                                </step>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <parameters>
                                        <parameter>1048576</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>edac</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#DIMM} - Errors Uncorrectable</name>
                            <type>DEPENDENT</type>
                            <key>edac.ue[{#DIMM}]</key>
                            <delay>0</delay>
                            <history>30d</history>
                            <application_prototypes>
                                <application_prototype>
                                    <name>EDAC</name>
                                </application_prototype>
                            </application_prototypes>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <parameters>
                                        <parameter>$.['{#DIMM}'].dimm_ue_count</parameter>
                                    </parameters>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>edac</key>
                            </master_item>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&gt;{$EDAC_UE_THRESH}</expression>
                                    <name>{#DIMM} - Uncorrectable errors detected: {ITEM.LASTVALUE}</name>
                                    <priority>HIGH</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                    <master_item>
                        <key>edac</key>
                    </master_item>
                    <lld_macro_paths>
                        <lld_macro_path>
                            <lld_macro>{#DIMM}</lld_macro>
                            <path>$.name</path>
                        </lld_macro_path>
                    </lld_macro_paths>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <parameters>
                                <parameter>$.*</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                </discovery_rule>
            </discovery_rules>
            <macros>
                <macro>
                    <macro>{$EDAC_CE_THRESH}</macro>
                    <value>0</value>
                </macro>
                <macro>
                    <macro>{$EDAC_UE_THRESH}</macro>
                    <value>0</value>
                </macro>
            </macros>
        </template>
    </templates>
</zabbix_export>
